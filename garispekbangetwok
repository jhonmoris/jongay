        <!-- Script dynamic RTP & Testimoni -->
        <script>
        document.addEventListener('DOMContentLoaded', function(){
          const tz = 'Asia/Jakarta';
          try {
            const now = new Date();
            const fmt = new Intl.DateTimeFormat('id-ID', { day:'2-digit', month:'short', year:'numeric', timeZone: tz });
            const el = document.getElementById('slot1000-rtp-date');
            if (el) el.textContent = fmt.format(now);
          } catch(e) {}

          function dayIndex() {
            const dt = new Date(new Date().toLocaleString('en-US', { timeZone: tz }));
            return Math.floor(dt.getTime() / 86400000);
          }

          function pickDaily(arr, take, step) {
            const idx = dayIndex();
            const out = [];
            const n = arr.length || 1;
            for (let i = 0; i < take; i++) {
              out.push(arr[(idx + i * step) % n]);
            }
            return out;
          }

          function renderRTP(games) {
            const table = document.getElementById('slot1000-rtp-table');
            if (!table) return;
            const tbody = table.querySelector('tbody');

            const normalized = games.map(g => {
              const r = Number(g.rtp || 0);
              const status = g.status || (r >= 96.8 ? 'GACOR' : 'STABIL');
              return { ...g, rtp:r, status };
            });

            const picked = pickDaily(normalized, 5, 3);
            tbody.innerHTML = picked.map((g, idx) => `
              <tr>
                <td>${idx + 1}</td>
                <td><div class="slot1000-rtp-name">${g.name || '-'}</div></td>
                <td class="slot1000-rtp-prov">${g.provider || '-'}</td>
                <td class="slot1000-rtp-num">${g.rtp ? g.rtp.toFixed(2) + '%' : '--'}</td>
                <td><span class="slot1000-badge ${g.status.toLowerCase()}">${g.status}</span></td>
              </tr>
            `).join('');
          }

          function renderTesti(items) {
            const table = document.getElementById('slot1000-testi-table');
            if (!table) return;
            const tbody = table.querySelector('tbody');

            const picked = pickDaily(items, 3, 2);
            tbody.innerHTML = picked.map((t, idx) => `
              <tr>
                <td>${idx + 1}</td>
                <td>
                  <div class="slot1000-testi-name">${t.name || '-'}</div>
                  <div class="slot1000-testi-stars"></div>
                </td>
                <td class="slot1000-testi-game">${t.game || '-'}</td>
                <td class="slot1000-testi-msg">${t.message || '-'}</td>
              </tr>
            `).join('');
          }

          fetch('/rtp.json', { cache:'no-store' })
            .then(r => r.json())
            .then(d => Array.isArray(d?.games) ? renderRTP(d.games) : null)
            .catch(()=>{});

          fetch('/testimoni.json', { cache:'no-store' })
            .then(r => r.json())
            .then(d => Array.isArray(d?.items) ? renderTesti(d.items) : null)
            .catch(()=>{});
        });
        </script>

      </div>
    </div>
  </div>
</div>

<!-- Proteksi basic (opsional, bisa dihapus kalau mengganggu) -->
<script>
(function () {
  const badCombos = new Set(['u','s','c','i']);
  document.addEventListener('keydown', function (e) {
    const k = (e.key || '').toLowerCase();
    if ((e.ctrlKey || e.metaKey) && badCombos.has(k)) e.preventDefault();
    if (k === 'f12') e.preventDefault();
  }, {passive:false});

  window.addEventListener('contextmenu', e => e.preventDefault());
  document.addEventListener('selectstart', function(e){
    const t = e.target;
    if (t.closest('input, textarea, [contenteditable="true"]')) return;
    e.preventDefault();
  });
  document.addEventListener('dragstart', e => e.preventDefault());
})();
</script>
<style>
html, body { -webkit-user-select:none; user-select:none; }
img { -webkit-user-drag:none; user-drag:none; }
</style>
</body>
</html>
